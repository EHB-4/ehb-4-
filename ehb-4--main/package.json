    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "auto-push": "node scripts/auto-push.js",
    "clean": "rimraf .next && rimraf node_modules/.cache",
    "analyze": "ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "agent-api": "cursor ai --workspace api-dev",
    "agent-ui": "cursor ai --workspace ui-dev",
    "agent-wallet": "cursor ai --workspace wallet-dev",
    "agents": "concurrently \"npm run agent-api\" \"npm run agent-ui\" \"npm run agent-wallet\"",
    "watch": "chokidar 'api/**/*' 'components/**/*' 'api/wallet/**/*' -c 'npm run dev'"
