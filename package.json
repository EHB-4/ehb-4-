{
  "name": "ehb-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "auto-push": "node scripts/auto-push.js",
    "clean": "rimraf .next && rimraf node_modules/.cache",
    "analyze": "ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "agent-api": "cursor ai --workspace api-dev",
    "agent-ui": "cursor ai --workspace ui-dev",
    "agent-wallet": "cursor ai --workspace wallet-dev",
    "agents": "concurrently \"npm run agent-api\" \"npm run agent-ui\" \"npm run agent-wallet\"",
    "watch": "chokidar 'api/**/*' 'components/**/*' 'api/wallet/**/*' -c 'npm run dev'"
  },
  "dependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/recharts": "^1.8.29",
    "chokidar": "^3.5.3",
    "critters": "^0.0.23",
    "framer-motion": "^12.17.3",
    "next": "15.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "recharts": "^2.12.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@swc/helpers": "^0.5.3",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "autoprefixer": "^10.4.17",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "15.3.3",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1"
  }
}
