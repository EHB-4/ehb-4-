{
  "name": "ehb-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "setup": "node scripts/setup.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cross-env PORT=3004 start-server-and-test 'npm run dev' http://localhost:3004 cypress:run",
    "test:e2e:dev": "cypress open",
    "test:e2e:wallet": "cross-env PORT=3004 start-server-and-test 'npm run dev' http://localhost:3004 cypress:run:wallet",
    "test:e2e:auth": "cross-env PORT=3004 start-server-and-test 'npm run dev' http://localhost:3004 cypress:run:auth",
    "prepare": "husky install",
    "security-audit": "npm audit && snyk test",
    "quality": "npm run lint && npm run test && npm run type-check && npm run security-audit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:wallet": "cypress run --spec 'cypress/e2e/wallet.cy.ts' --group wallet",
    "cypress:run:auth": "cypress run --spec 'cypress/e2e/auth.cy.ts' --group auth",
    "cypress:record": "cypress run --record",
    "cypress:record:parallel": "cypress run --record --parallel --ci-build-id $CIRCLE_BUILD_NUM || cypress run --record --parallel --ci-build-id local-$(date +%s)",
    "cypress:report": "mochawesome-merge cypress/reports/*.json > cypress/reports/report.json && marge cypress/reports/report.json -f report -o cypress/reports"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.9.1",
    "@auth/prisma-adapter": "^2.9.1",
    "@aws-sdk/client-s3": "^3.826.0",
    "@easypost/api": "^8.1.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@headlessui/react": "^2.2.4",
    "@onfido/api": "^5.2.0",
    "@polkadot/api": "^16.2.1",
    "@polkadot/keyring": "^13.5.1",
    "@polkadot/util-crypto": "^13.5.1",
    "@prisma/client": "^5.22.0",
    "@react-google-maps/api": "^2.20.6",
    "@sendgrid/mail": "^8.1.5",
    "@shadcn/ui": "^0.0.4",
    "@shopify/shopify-api": "^11.13.0",
    "@stripe/stripe-js": "^7.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/mongodb": "^4.0.6",
    "@types/mongoose": "^5.11.96",
    "@types/next-auth": "^3.13.0",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.0",
    "algoliasearch": "^5.27.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.9",
    "cohere": "^1.1.1",
    "ethers": "^5.7.2",
    "framer-motion": "^11.18.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "mapbox-gl": "^3.12.0",
    "messagebird": "^4.0.1",
    "mongodb": "^6.17.0",
    "mongoose": "^8.15.1",
    "next": "14.1.0",
    "next-auth": "^4.24.6",
    "openai": "^5.3.0",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.3",
    "redis": "^3.1.2",
    "shippo": "^2.15.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.0",
    "twilio": "^5.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.80.7",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "cypress": "^13.17.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.2.0",
    "msw": "^2.2.1",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "prisma": "^5.10.2",
    "snyk": "^1.1264.0",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "jest --bail --findRelatedTests"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
