# NPM Configuration for EHB Next.js Platform
legacy-peer-deps=true
package-lock=true
save-exact=false
audit=false
fund=false
loglevel=warn

# Prevent common issues
ignore-scripts=false
unsafe-perm=true

# Cache settings
cache=.npm-cache
prefer-offline=true

# Registry settings
registry=https://registry.npmjs.org/
